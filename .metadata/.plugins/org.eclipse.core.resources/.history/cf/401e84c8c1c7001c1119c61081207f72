package linkedlist;

public class SinglyLinkedList {
	
	class Node
	{
		int data;
		Node next;
		
		
		
		public Node(int data) {
			super();
			this.data = data;
			this.next = null;
		}
	}
	
	
		Node head;
		
		void insert(int element)
		{
			Node newNode = new Node(element);
			if(head==null)
			{
				head=newNode;
			}
			else
			{
				Node iterator=head;
				while(iterator.next!=null)
				{
					iterator=iterator.next;
				}
				iterator.next=newNode;
			}
		}
		
		
		void delete()
		
}
